{
    "name": "blair2004/nexopos",
    "type": "project",
    "description": "The Free Modern Point Of Sale System build with Laravel, TailwindCSS and Vue.js.",
    "keywords": [
        "nexopos",
        "point of sale",
        "pos",
        "pos system"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "ext-sqlite": "*",
        "beyondcode/laravel-websockets": "^1.11",
        "dekor/php-array-table": "^1.0",
        "doctrine/dbal": "^3.0",
        "fakerphp/faker": "^1.12",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "gumlet/php-image-resize": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jackiedo/dotenv-editor": "^1.1",
        "laravel/framework": "^8.0",
        "laravel/sanctum": "^2.14",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "laravie/parser": "^2.1",
        "phpoffice/phpspreadsheet": "^1.17",
        "picqer/php-barcode-generator": "^2.1",
        "pusher/pusher-php-server": "^7.0",
        "spatie/laravel-db-snapshots": "^1.7",
        "tormjens/eventy": "^0.7.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi",
            "@php artisan storage:link --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan storage:link --ansi"
        ]
    }
}
